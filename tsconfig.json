{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-native",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@screens/*": ["screens/*"],
      "@utils/*": ["utils/*"],
      "@colors": ["styles/colors"],
      "@authService": ["services/firebase/authService"],
      "@navigationService":["services/navigation/navigationService"],
      "@navigationTypes": ["types/navigationTypes"],
      "@types/*" :["types/*"]
    }
  }
}
